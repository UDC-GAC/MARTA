for (i = 42; i <= 43; ++i)
   for (j = 69; j <= 70; ++j)
      y[4*i + 1024] += A[32*i + 4096] * x[64*j + (i-j)*172 + 1234];
for (i = 44; i <= 45; ++i)
  for (j = 0; j <= 1; ++j)
      y[16*i + 1024] += A[16*i + 4096] * x[16*j + 4567];

// individual total cost: 32 cycles
// fused cost: 21.06 cycles
